{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      },
      "gopls": {
        "semanticTokens": false
      }
    }
  },
  "html.filetypes": ["html", "handlebars", "htmldjango", "blade", "vue"]
}
